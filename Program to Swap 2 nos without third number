
import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Input the first element");
	    int a =sc.nextInt();
	    System.out.println("Input the Second element");
	    int b=sc.nextInt();
	    
	    System.out.println("The Number before swapping are");
	    System.out.println(a +"\t" +b);
	    
	    
		
		a=a+b;
		b=a-b;
		a=a-b;
		System.out.println("THe numbers after swapping are " + a + b);
		
	}
	
	
}
